# temp.civet

import {Node} from 'npm:typescript'

import {
	undef, defined, notdefined, LOG, DIR, FAIL, SUCCEED,
	buildFileName, findFile, sep, centered, TConstructor,
	} from 'base-utils'
import {o, hasKey, hasKeys} from 'llutils'
import {getCmdArgs} from 'cmd-args'
import {civet2ast} from 'civet'
import {
	astAsString, kindStr, AstWalker,
	} from 'typescript'

getCmdArgs()

# ---------------------------------------------------------------------------

hAst := civet2ast """
	export x := 42
	export f := (i: number): string =>
		return i.toString()
	y := 13
	"""

LOG astAsString(hAst)

walker := new AstWalker()

# --- Attempt to list out all exports
LOG sep()
for {node} of walker.walk(hAst)
	kind := kindStr(node.kind)
	LOG kind
