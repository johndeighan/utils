# temp2.civet

import {
	undef, defined, notdefined,
	} from 'datatypes'
import {
	escapeStr,
	} from 'llutils'
import {
	setLogLevel, LOG, pushLogLevel, popLogLevel,
	} from 'logger'
import {
	getParser, ALL, ANY,
	} from 'parser'

# setLogLevel 'debug'
let parse = getParser(/a/)

# ---------------------------------------------------------------------------

test := (str: string): void =>

	try
		parse(str)
		LOG "#{escapeStr(str)}"
	catch err
		LOG "FAIL #{escapeStr(str)}"
	return

# ---------------------------------------------------------------------------

reIdent := ///^
	[a-zA-Z_$]
	[a-zA-Z0-9_$]*
	///
reIdent.toString = () => 'reIdent'

reNumber := ///^
	[-+]?
	\d+
	(?:
		\.
		\d+
		([eE][-+]?\d+)?
		)?
	///
reNumber.toString = () => 'reNumber'

reString := ///^
	(?: ' ([^']*) ' ) | (?: " ([^"]*) " )
	///
reString.toString = () => 'reString'

